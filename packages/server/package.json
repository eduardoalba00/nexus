    "drizzle-orm": "^0.44.1",
    "fastify": "^5.3.3",
    "jose": "^6.0.11",
    "livekit-server-sdk": "^2.9.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
